{
    "manifest_version": 2,
 
    "name": "Show Pasword Plugin",
    "description": "This extension will show password in password fields",
    "version": "1.0",
 
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "<all_urls>",
        "storage"
    ]
}